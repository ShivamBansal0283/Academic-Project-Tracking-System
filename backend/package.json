{
  "name": "apts-server-prisma",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "build": "prisma generate && tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "start:prod": "prisma migrate deploy && node dist/index.js",
